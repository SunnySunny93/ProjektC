<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<appSettings>
    	<!-- <add key="Server" value="127.0.0.1" />-->
  </appSettings>
  <system.diagnostics>
  <trace autoflush="true" indentsize="4">
    <listeners>
      <add name="myListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="log.txt" traceOutputOptions="DateTime" />
      <remove name="Default" />
    </listeners>
  </trace>
</system.diagnostics>
</configuration>
